/* pages/mine/mine.wxss */
view,text{
  letter-spacing: 2rpx;
}

.top {
  text-align: center;
  font-size: 30rpx;
  padding: 80rpx 0 60rpx;
}

.head-logo {
  width: 180rpx;
  height: 180rpx;
  border-radius: 90rpx;
  overflow: hidden;
  margin: 0 auto;
}
button[type="primary"]{
  vertical-align: middle;
  text-align: center;
  width: 40%;
  height: 50%;
  font-size: 30rpx;
  border-radius:40rpx; 

}
.row {
  height: 13rpx;
  background-color: #f3f3f3;
  border-top: 1rpx solid #eaeaea;
  border-bottom: 1rpx solid #eaeaea;
  padding: 0 30rpx;
}

.manage{
  text-align: center;
  font-size:26rpx;
  margin: 40rpx auto;
  /* margin:63rpx 0 33rpx; */
  padding-left: 10rpx;
}
.manage navigator{
  /* flex:1; */
  float: left;
  margin: 12rpx 10rpx;
  
}
.manage button{
  font:400 28rpx/64rpx 'microsoft yahei';
}

.btn_hover{
  background-color:#f3f3f3;
  color: #000;
}